# Zadanie 1 


def policz_wielomian(n):
    min_mnozen = n
    min_dodan = n
    max_mnozen = n * (n + 1) // 2
    max_dodan = n

    print("Najmniej mnożeń:", min_mnozen)
    print("Najmniej dodawań:", min_dodan)
    print("Najwięcej mnożeń:", max_mnozen)
    print("Najwięcej dodawań:", max_dodan)

policz_wielomian(5)
